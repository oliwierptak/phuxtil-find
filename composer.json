{
    "name": "phuxtil/find",
    "description": "Easy interface for output of 'find' unix command",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "phuxtil/chmod": "^2",
        "phuxtil/splfileinfo": "^2"
    },
    "require-dev": {
        "symfony/var-dumper": "^4",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Phuxtil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhuxtilTests\\Functional\\": "tests/Functional"
        }
    }
}
